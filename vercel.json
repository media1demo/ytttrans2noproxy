{
  "version": 2,
  "builds": [
    {
      "src": "api/get_transcript.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/transcript",  
      "dest": "/api/get_transcript.py"
    },
    {
      "src": "/(.*)",          
      "dest": "/public/$1"     
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" } 
  ]
}
